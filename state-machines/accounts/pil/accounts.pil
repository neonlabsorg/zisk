require "std_lookup.pil"

const int ACCOUNTS_BUS_ID = 7891;

public accounts_root[4];

airtemplate Accounts(int N = 2**21, int stack_enabled = 0, const int accounts_bus_id = ACCOUNTS_BUS_ID, const int RC=2) {
    commit stage(0) public(accounts_root) accounts;

    col accounts addr;
    col accounts val_high;
    col accounts val_low;

    col witness multiplicity;

    lookup_proves(accounts_bus_id, [addr, val_high, val_low], mul: multiplicity);
}
